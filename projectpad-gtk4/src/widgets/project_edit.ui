<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="ProjectEdit" parent="AdwBin">
    <child>
      <object class="AdwBin" id="project_item">
        <child>
          <object class="GtkBox">
            <property name="width-request">400</property>
            <property name="orientation">vertical</property>
            <property name="spacing">20</property>
            <property name="margin_start">10</property>
            <property name="margin_end">10</property>
            <property name="margin_bottom">10</property>
            <property name="margin_top">10</property>
            <child>
              <object class="GtkBox" id="header_box">
                <property name="css-classes">toolbar</property>
                <child>
                  <object class="GtkImage">
                    <property name="pixel-size">48</property>
                    <property name="icon-name">cubes</property>
                  </object>
                </child>
                <child>
                  <object class="GtkBox" id="header_second_col">
                    <property name="orientation">vertical</property>
                    <property name="spacing">20</property>
                    <property name="valign">center</property>
                    <property name="hexpand">True</property>
                    <child>
                      <object class="GtkEntry" id="project_name">
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="EnvironmentListPicker" id="env_picker">
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
