<?xml version="1.0" encoding="utf-8"?>
<interface>
  <object class="GtkShortcutsWindow" id="shortcuts">
    <property name="modal">1</property>
    <child>
      <object class="GtkShortcutsSection">
        <property name="section-name">shortcuts</property>
        <property name="max-height">12</property>
        <property name="visible">1</property>
        <child>
          <object class="GtkShortcutsGroup">
            <property name="visible">1</property>
            <property name="title" translatable="yes">Main view</property>
            <property name="view">events</property>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">ltr</property>
                <property name="accelerator">&lt;ctl&gt;e</property>
                <property name="title" translatable="yes">Edit displayed item</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">rtl</property>
                <property name="accelerator">&lt;ctl&gt;e</property>
                <property name="title" translatable="yes">Edit displayed item</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">ltr</property>
                <property name="accelerator">&lt;ctl&gt;y</property>
                <property name="title" translatable="yes">Copy password</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">rtl</property>
                <property name="accelerator">&lt;ctl&gt;y</property>
                <property name="title" translatable="yes">Copy password</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">ltr</property>
                <property name="accelerator">&lt;ctl&gt;s</property>
                <property name="title" translatable="yes">Activate search mode (can also just type)</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">rtl</property>
                <property name="accelerator">&lt;ctl&gt;s</property>
                <property name="title" translatable="yes">Activate search mode (can also just type)</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkShortcutsGroup">
            <property name="visible">1</property>
            <property name="title" translatable="yes">Search view</property>
            <property name="view">events</property>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">ltr</property>
                <property name="accelerator">Escape</property>
                <property name="title" translatable="yes">Leave search mode</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">rtl</property>
                <property name="accelerator">Escape</property>
                <property name="title" translatable="yes">Leave search mode</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">ltr</property>
                <property name="accelerator">Return</property>
                <property name="title" translatable="yes">Open single search result</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">rtl</property>
                <property name="accelerator">Return</property>
                <property name="title" translatable="yes">Open single search result</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkShortcutsGroup">
            <property name="visible">1</property>
            <property name="title" translatable="yes">Note display and editing</property>
            <property name="view">events</property>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">ltr</property>
                <property name="accelerator">&lt;Ctl&gt;f</property>
                <property name="title" translatable="yes">Search</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">rtl</property>
                <property name="accelerator">&lt;Ctl&gt;f</property>
                <property name="title" translatable="yes">Search</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">ltr</property>
                <property name="accelerator">&lt;Ctl&gt;n</property>
                <property name="title" translatable="yes">Next search match</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">rtl</property>
                <property name="accelerator">&lt;Ctl&gt;n</property>
                <property name="title" translatable="yes">Next search match</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">ltr</property>
                <property name="accelerator">Return</property>
                <property name="title" translatable="yes">Next search match</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">rtl</property>
                <property name="accelerator">Return</property>
                <property name="title" translatable="yes">Next search match</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">ltr</property>
                <property name="accelerator">&lt;Ctl&gt;p</property>
                <property name="title" translatable="yes">Previous search match</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="direction">rtl</property>
                <property name="accelerator">&lt;Ctl&gt;p</property>
                <property name="title" translatable="yes">Previous search match</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
